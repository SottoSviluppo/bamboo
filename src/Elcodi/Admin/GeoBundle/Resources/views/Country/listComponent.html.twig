{% extends 'AdminCoreBundle::Common/listComponent.html.twig' %}


{% set elcodi_context = {
    entity_type: 'country',
} %}


{% block title 'admin.country.list'|trans %}


{% block no_items
    lists.no_items(
        'admin.country.single'|trans,
        'admin.country.plural'|trans,
        url('admin_country_new')
    )
%}


{% block table_head %}
	{% include 'AdminGeoBundle::Country/tableHead.html.twig' %}
{% endblock table_head %}


{% block table_body_row %}
	{% include 'AdminGeoBundle::Country/tableRow.html.twig' with {'entity': entity} %}
{% endblock table_body_row %}


{% block table_paginator_links %}

    {% set url = 'admin_country_list' %}
    {{ parent() }}

{% endblock table_paginator_links %}
