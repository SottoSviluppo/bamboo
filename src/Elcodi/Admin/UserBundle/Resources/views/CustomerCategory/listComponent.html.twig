{% block no_items lists.no_items('admin.category.single' | trans, 'admin.category.plural' | trans, path('admin_customer_category_new')) %}
        
{% block table_head %}
    {% include 'AdminUserBundle::CustomerCategory/tableHead.html.twig' %}
{% endblock table_head %}
{% block table_body_row %}
