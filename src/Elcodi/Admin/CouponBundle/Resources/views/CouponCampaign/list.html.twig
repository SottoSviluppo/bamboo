{% extends 'AdminCoreBundle::layout.html.twig' %}
{% set elcodi_context = {
    entity_type: 'coupon_campaign'
} %}
{% block breadcrumb %}
    {% include '@AdminCore/Common/breadcrumb.html.twig' with {
        breadcrumb : [
            {
                name: 'admin.plugin.single' | trans,
                active: false
            }, {
                name: 'admin.coupon_campaign.plural' | trans,
                active: true
            }
        ]
    } %}
{% endblock breadcrumb %}
{% block header_actions %}
    <a href="{{ url('admin_coupon_campaign_new') }}" class="button-primary">
        <i class="icon-plus"></i>
        <span>{{ 'admin.coupon_campaign.new'|trans }}</span>
    </a>
{% endblock header_actions %}
{% block content %}
    <div class="row">
        <div class="col-md-12">
            {{
                render(
                    url('admin_coupon_campaign_list_component', {
                        page: page,
                        limit: limit,
                        orderByField: orderByField,
                        orderByDirection: orderByDirection,
                    })
                )
            }}
        </div>
    </div>
{% endblock content %}