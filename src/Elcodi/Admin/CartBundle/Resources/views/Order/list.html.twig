{% extends 'AdminCoreBundle::layout.html.twig' %}


{% set elcodi_context = {
    entity_type: 'order',
} %}


{% block breadcrumb %}

    {% include '@AdminCore/Common/breadcrumb.html.twig' with {
        breadcrumb: [
            { name: 'admin.order.plural'|trans, active: true }
        ],
    } %}

{% endblock breadcrumb %}


{% block content %}
    {{
        render(url('search_admin_component_orders', {
            query: query,
            page: page,
            limit: limit,
            dateFrom: dateFrom,
            dateTo: dateTo,
            orderState: orderState,
            coupon: coupon,
            couponCampaign: couponCampaign,
            invoiceRequested: invoiceRequested,
            countryId: countryId,
            shippingState: shippingState,
            customerEmail: customerEmail,
            paymentMethod: paymentMethod,
            idFrom: idFrom,
            idTo: idTo,
            submit: submit,
            template: template,
        }))
    }}

{% endblock content %}
