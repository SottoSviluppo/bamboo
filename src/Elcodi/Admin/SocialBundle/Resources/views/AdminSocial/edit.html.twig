{% extends 'AdminCoreBundle::layout.html.twig' %}


{% set elcodi_context = {
    entity_type: 'social',
} %}



{% block breadcrumb %}

	{% set title = 'admin.social.plural'|trans %}
	{%
        set header = social.id
            ? 'admin.social.edit'|trans({ '%name%': social.title })
            : 'admin.social.new'|trans
    %}

	{% include '@AdminCore/Common/breadcrumb.html.twig' with {
        breadcrumb: [
            { name: 'admin.communication.single'|trans, active: false },
            { name: title, url: url('admin_social_list'), },
            { name: header, active: true, },
        ]
    } %}

{% endblock breadcrumb %}


{% block content %}

	{% if social.id %}
        {{
            render(url('admin_social_edit_component', {
                id: social.id,
            }))
        }}
	{% else %}
        {{ render(url('admin_social_new_component')) }}
	{% endif %}

{% endblock content %}
