{% extends 'AdminCoreBundle::layout.html.twig' %}


{% set elcodi_context = {
    entity_type: 'tax',
} %}


{% block header_actions %}

{% endblock header_actions %}


{% block breadcrumb %}

	{% set title = 'admin.general.plural'|trans %}
	{%
        set header = tax.id
            ? 'admin.general.edit'|trans({ '%name%': tax.title })
            : 'admin.general.new'|trans
    %}

	{% include '@AdminCore/Common/breadcrumb.html.twig' with {
        breadcrumb: [
            { name: 'admin.communication.single'|trans, active: false },
            { name: title, url: url('admin_tax_list'), },
            { name: header, active: true, },
        ]
    } %}

{% endblock breadcrumb %}


{% block content %}

	{% if tax.id %}
        {{
            render(url('admin_tax_edit_component', {
                id: tax.id,
            }))
        }}
	{% else %}
        {{ render(url('admin_tax_new_component')) }}
	{% endif %}

{% endblock content %}
