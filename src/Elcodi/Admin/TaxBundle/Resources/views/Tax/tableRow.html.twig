        </div>
    </td>
    <td>
        <div class="switch">
            <input id="enable-{{ entity.id }}" type="hidden" value="{{ path('admin_tax_enable', { id: entity.id }) }}" />
            <input id="disable-{{ entity.id }}" type="hidden" value="{{ path('admin_tax_disable', { id: entity.id }) }}" />
            <input id="{{ entity.id }}" type="checkbox" {% if entity.enabled %}checked="checked"{% endif %} />
            <label for="{{ entity.id }}"></label>
        </div>
    </td>


    {% include '@AdminCore/Tables/actions.html.twig' with {
