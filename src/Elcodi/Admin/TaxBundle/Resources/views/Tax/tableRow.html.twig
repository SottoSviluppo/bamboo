    <td>
        <a href="{{ path('admin_tax_edit', { id: entity.id }) }}">
            <strong>{{ entity.name|truncate(20, 'â€¦') }}</strong>
        </a>
    </td>
    <td>
        <div class="switch">
            <input id="enable-{{ entity.id }}-master" type="hidden" value="{{ path('admin_tax_master', { id: entity.id }) }}" />
